<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nokee.android_image_processing" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrimaryColor"
            android:label="@string/title_activity_primary_color" >
        </activity>
        <activity
            android:name=".ColorMatrix"
            android:label="@string/title_activity_color_matrix" >
        </activity>
        <activity
            android:name=".PixelsEffect"
            android:label="@string/title_activity_pixels_effect" >
        </activity>
    </application>

</manifest>
